description=style behavior reference (htc)
servletPath=.

matchesScripts=false
matchesTags=true
# capturing group 1 = quote char // capturing group 2 = URL 
urlCapturingPattern@prefilter=behavior
urlCapturingPattern=(?i)(?s)style\\s*\=\\s*('|"|`)(?\:.*?)behavior\\s*\:\\s*url\\s*\\(([^\\)]{0,2500})\\)
capturingGroupNumbers=2
urlExclusionPattern=

# those exclusions are matched against the complete tag/script (as always using find())
tagExclusionPattern=
scriptExclusionPattern=
